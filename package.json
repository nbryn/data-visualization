{
   "name": "statsboard",
   "version": "1.0.0",
   "description": "Dashboard",
   "main": "index.js",
   "engines": {
      "npm": "6.12.0",
      "node": "12.13.0"
   },
   "scripts": {
      "start": "node index.js",
      "server": "ts-node index.js && nodemon index.js",
      "client": "npm start --prefix presentation",
      "dev": "concurrently \"npm run server\" \"npm run client\" ",
      "prettier": "prettier --config .prettierrc 'logic/**/*.ts' --write",
      "heroku-postbuild": "cd presentation && npm install && npm run build"
   },
   "repository": {
      "type": "git",
      "url": "git+https://https://github.com/nbryn/dashboard.git"
   },
   "author": "nbryn",
   "license": "ISC",
   "homepage": "",
   "dependencies": {
      "@types/mongoose": "^5.7.32",
      "apollo-server": "^2.9.12",
      "apollo-server-express": "^2.9.12",
      "axios": "^0.19.0",
      "cors": "^2.8.5",
      "country-codes-list": "^1.6.5",
      "country-currency-map": "^2.1.7",
      "dotenv": "^8.2.0",
      "graphql": "^14.5.8",
      "graphql-tools": "^4.0.6",
      "graphql-type-json": "^0.3.1",
      "iso-country-currency": "^0.5.1",
      "lodash": "^4.17.15",
      "moment": "^2.24.0",
      "mongoose": "^5.7.12",
      "ts-node": "^9.0.0",
      "typescript": "^3.9.7"
   },
